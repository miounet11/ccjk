#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@22.18.6_change-case@5.4.4_pos_51a1cee1d3aaf699f5e520ef6bacc566/node_modules/vitepress/bin/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@22.18.6_change-case@5.4.4_pos_51a1cee1d3aaf699f5e520ef6bacc566/node_modules/vitepress/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@22.18.6_change-case@5.4.4_pos_51a1cee1d3aaf699f5e520ef6bacc566/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@22.18.6_change-case@5.4.4_pos_51a1cee1d3aaf699f5e520ef6bacc566/node_modules/vitepress/bin/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@22.18.6_change-case@5.4.4_pos_51a1cee1d3aaf699f5e520ef6bacc566/node_modules/vitepress/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.42.0_@types+node@22.18.6_change-case@5.4.4_pos_51a1cee1d3aaf699f5e520ef6bacc566/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
