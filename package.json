    "type": "git",
    "url": "git+https://github.com/miounet11/ccjk.git"
  },
  "bugs": "https://github.com/miounet11/ccjk/issues",
  "keywords": [
    "ai",
    "code-tools",
    "abstraction",
    "claude",
    "claude-code",
    "codex",
    "ccjk",
    "jinku",
    "ai-assistant",
    "ai-coding",
    "code-assistant",
    "developer-tools",
    "devtools",
