{
  "name": "ccjk",
  "type": "module",
  "version": "2.4.4",
  "packageManager": "pnpm@10.17.1",
  "description": "Claude Code JinKu - Advanced AI-powered development assistant with skills, agents, and LLM-driven audit",
  "author": {
    "name": "CCJK Team"
  },
  "license": "MIT",
  "homepage": "https://github.com/miounet11/ccjk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miounet11/ccjk.git"
  },
  "bugs": "https://github.com/miounet11/ccjk/issues",
  "keywords": [
    "claude",
    "claude-code",
    "codex",
    "ccjk",
    "jinku",
    "ai-assistant",
    "ai-coding",
    "code-assistant",
    "developer-tools",
    "devtools",
    "cli",
    "automation",
    "mcp",
    "anthropic",
    "openai",
    "gemini",
    "gpt",
    "llm",
    "machine-learning",
    "artificial-intelligence",
    "skills",
    "agents",
    "subagents",
    "shencha",
    "audit",
    "code-review",
    "security-audit",
    "devops",
    "cicd",
    "testing",
    "documentation",
    "refactoring",
    "plugin-system",
    "typescript",
    "javascript",
    "python",
    "react",
    "vue",
    "nextjs",
    "nodejs",
    "fullstack",
    "backend",
    "frontend",
    "api",
    "microservices",
    "docker",
    "kubernetes",
    "performance",
    "optimization",
    "best-practices",
    "clean-code",
    "productivity",
    "workflow",
    "cursor-alternative",
    "copilot-alternative",
    "codeium-alternative"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "bin": {
    "ccjk": "bin/ccjk.mjs"
  },
  "files": [
    "bin",
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx ./src/cli.ts",
    "build": "unbuild",
    "start": "node bin/ccjk.mjs",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "node scripts/prepublish.mjs && pnpm build",
    "postpublish": "node scripts/postpublish.mjs",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "changeset": "changeset",
    "version": "changeset version",
    "update:deps": "pnpx taze major -r -w",
    "release": "pnpm build && changeset publish",
    "prepare": "husky",
    "commitlint": "commitlint",
    "commitlint:check": "commitlint --from HEAD~1 --to HEAD --verbose",
    "docs:dev": "pnpm -F @ccjk/docs dev",
    "docs:build": "pnpm -F @ccjk/docs build",
    "docs:preview": "pnpm -F @ccjk/docs preview"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@iarna/toml": "^2.2.5",
    "@types/semver": "^7.7.1",
    "@types/tar": "^6.1.13",
    "ansis": "^4.1.0",
    "cac": "^6.7.14",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "commander": "^14.0.2",
    "dayjs": "^1.11.18",
    "find-up-simple": "^1.0.1",
    "fs-extra": "^11.3.2",
    "gray-matter": "^4.0.3",
    "i18next": "^25.5.2",
    "i18next-fs-backend": "^2.6.0",
    "inquirer": "^12.9.6",
    "inquirer-toggle": "^1.0.1",
    "nanoid": "^5.1.6",
    "ora": "^9.0.0",
    "pathe": "^2.0.3",
    "semver": "^7.7.2",
    "smol-toml": "^1.4.2",
    "tar": "^7.5.2",
    "tinyexec": "^1.0.1",
    "trash": "^10.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.18.6",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "^1.0.2",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*": [
      "pnpm lint"
    ]
  }
}
