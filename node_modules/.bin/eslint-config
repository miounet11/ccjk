#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules/@antfu/eslint-config/bin/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules/@antfu/eslint-config/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules/@antfu/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules/@antfu/eslint-config/bin/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules/@antfu/eslint-config/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules/@antfu/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/@antfu+eslint-config@5.4.1_@vue+compiler-sfc@3.5.25_eslint-plugin-format@1.0.2_eslint@9_64b0828d61051caa289fe01d22b8eb00/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
else
  exec node  "$basedir/../@antfu/eslint-config/bin/index.js" "$@"
fi
