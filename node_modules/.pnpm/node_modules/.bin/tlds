#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/tlds@1.261.0/node_modules/tlds/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/tlds@1.261.0/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/tlds@1.261.0/node_modules/tlds/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/tlds@1.261.0/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tlds/bin.js" "$@"
else
  exec node  "$basedir/../tlds/bin.js" "$@"
fi
