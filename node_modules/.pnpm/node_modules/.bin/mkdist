#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/mkdist@2.3.0_typescript@5.9.2_vue@3.5.25_typescript@5.9.2_/node_modules/mkdist/dist/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/mkdist@2.3.0_typescript@5.9.2_vue@3.5.25_typescript@5.9.2_/node_modules/mkdist/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/mkdist@2.3.0_typescript@5.9.2_vue@3.5.25_typescript@5.9.2_/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lu/ccjk-public/node_modules/.pnpm/mkdist@2.3.0_typescript@5.9.2_vue@3.5.25_typescript@5.9.2_/node_modules/mkdist/dist/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/mkdist@2.3.0_typescript@5.9.2_vue@3.5.25_typescript@5.9.2_/node_modules/mkdist/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/mkdist@2.3.0_typescript@5.9.2_vue@3.5.25_typescript@5.9.2_/node_modules:/Users/lu/ccjk-public/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdist/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../mkdist/dist/cli.cjs" "$@"
fi
